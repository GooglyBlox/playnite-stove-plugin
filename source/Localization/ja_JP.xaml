<?xml version="1.0"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- StoveLibrary -->
    <sys:String x:Key="LOCStoveLibrary">STOVE ライブラリ</sys:String>
    <sys:String x:Key="LOCStoveAuthError">STOVE との認証に失敗しました！</sys:String>
    <sys:String x:Key="LOCStoveImportError">STOVE ライブラリのインポートエラー！</sys:String>
    
    <!-- StoveLibrarySettingsView -->
    <sys:String x:Key="LOCStoveProfileUrl">プロフィール URL：</sys:String>
    <sys:String x:Key="LOCStoveProfileUrlPlaceholder">https://profile.onstove.com/ja/237901057/</sys:String>
    <sys:String x:Key="LOCStoveProfileUrlEmpty">プロフィール URL は空にできません。</sys:String>
    <sys:String x:Key="LOCStoveProfileUrlInvalid">プロフィール URL は次の形式である必要があります：https://profile.onstove.com/[言語]/[ユーザーID]/</sys:String>
    <sys:String x:Key="LOCStoveImportMetadata">メタデータをインポート</sys:String>
    <sys:String x:Key="LOCStoveImportTags">タグをインポート</sys:String>
    <sys:String x:Key="LOCStoveAllowAdult">18+ STOVE タイトルを許可</sys:String>
    <sys:String x:Key="LOCStoveAllowAdultTooltip">チェックすると、プラグインは ADULT_GAME_AGREE Cookie を事前に設定し、確認ページなしで大人向けゲームをスキャンできます。</sys:String>
    
    <!-- StoveClient -->
    <sys:String x:Key="LOCStoveClientNotFound">STOVE クライアントが見つかりません！</sys:String>
    
    <!-- StoveLibraryExeSelection -->
    <sys:String x:Key="LOCStoveLibraryExeSelectionTitle">レイアクション用の実行ファイルを選択</sys:String>
</ResourceDictionary>