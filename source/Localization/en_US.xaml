<?xml version="1.0"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- StoveLibrary -->
    <sys:String x:Key="LOCStoveLibrary">STOVE Library</sys:String>
    <sys:String x:Key="LOCStoveAuthError">Authentication with STOVE failed!</sys:String>
    <sys:String x:Key="LOCStoveImportError">Error importing STOVE library!</sys:String>
    
    <!-- StoveLibrarySettingsView -->
    <sys:String x:Key="LOCStoveProfileUrl">Profile URL:</sys:String>
    <sys:String x:Key="LOCStoveImportMetadata">Import metadata</sys:String>
